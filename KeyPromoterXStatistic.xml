<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'⌘X' or '⇧⌦'</ShortCut>
          <Description>Cut</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Extend Selection">
        <Item>
          <IdeaActionID>EditorSelectWord</IdeaActionID>
          <ShortCut>'⌥E'</ShortCut>
          <Description>Extend Selection</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move...">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'⇧⌘K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>25</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'⌘7'</ShortCut>
          <Description>Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Undo Move">
        <Item>
          <IdeaActionID>$Undo</IdeaActionID>
          <ShortCut>'⌘Z'</ShortCut>
          <Description>Undo Move</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Undo Renaming file chocolate_totals...">
        <Item>
          <IdeaActionID>$Undo</IdeaActionID>
          <ShortCut>'⌘Z'</ShortCut>
          <Description>Undo Renaming file chocolate_totals...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="VCS Operations Popup...">
        <Item>
          <IdeaActionID>Vcs.QuickListPopupAction</IdeaActionID>
          <ShortCut>'⌃V'</ShortCut>
          <Description>VCS Operations Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>